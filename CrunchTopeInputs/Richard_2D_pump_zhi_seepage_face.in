TITLE
Richard_2D_tracer
END

RUNTIME
time_units       seconds
timestep_max     1000
timestep_init    0.0001
time_tolerance   0.01

hindmarsh        false
correction_max   1
debye-huckel     yes
master           Tracer
database_sweep   no
speciate_only    no
gimrt            true
database         Datacom.dbs
screen_output    1000
solver           direct
END

OUTPUT
time_units               years
spatial_profile          0.0001
!!!0.1  0.2
END

TRANSPORT
!!distance_units meters
!!time_units     seconds
!!fix_diffusion  1e-9
!!gas_diffusion  9E-06
!!MilligtonQuirk true
!!dispersivity   0.0

FLOW
time_units       years
distance_units   meters
co_richards      0.9
calculate_flow   TRUE
!!PUMP
 Pump 0.00033975 injection 1 2 1
 Pump 0.00033975 injection 2 3 1
 Pump 0.00033975 injection 3 4 1
 Pump 0.00033975 injection 4 5 1
 Pump 0.00033975 injection 5 6 1
 Pump 0.00033975 injection 6 7 1
 Pump 0.00033975 injection 7 8 1
 Pump 0.00033975 injection 8 9 1
 Pump 0.00033975 injection 9 10 1
 Pump 0.00033975 injection 10 11 1
 Pump 0.00033975 injection 11 12 1
 Pump 0.00033975 injection 12 13 1
 Pump 0.00033975 injection 13 14 1
 !Pump 0.00033975 injection 14 15 1
 !Pump 0.00033975 injection 15 16 1

!!PERMEABILITY
permeability_x 1e-12 default
permeability_y 1e-12 default
permeability_z 1.418e-30 default

permeability_y 0 zone 1-15 0-0 1-1
permeability_y 0 zone 1-15 21-21 1-1

permeability_x 0 zone 0-0 1-20 1-1
permeability_x 1.e-12 zone 16-16 1-20 1-1

upstream_weighting TRUE
!!RICHARD
ns_solve           FALSE
Richards           TRUE
redistribute_wc    TRUE
!!wc_init          0.05
wc_residual        0.001
watertable_init    0.0
vangenuchten_alpha 7.5
vangenuchten_n     1.34
y_is_vertical      TRUE

!!PRESSURE
 pressure  0      default
 pressure  0.0    zone 1-15 1-1 1-1 fix
