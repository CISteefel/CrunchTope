TITLE
Richard_1D_Forsyth1995
END

RUNTIME
time_units       seconds
timestep_max     10000
timestep_init    0.0001
time_tolerance   0.01
hindmarsh        false
correction_max   1
debye-huckel     yes
master           Tracer
database_sweep   no
speciate_only    no
gimrt            true
database         Datacom.dbs
screen_output    1000
solver           direct
END

OUTPUT
time_units               years
spatial_profile          0.0027 0.011 0.055 0.274
END

TRANSPORT
!!distance_units meters
!!time_units     seconds
!!fix_diffusion  1e-9
!!gas_diffusion  9E-06
!!MilligtonQuirk true
!!dispersivity   0.0

FLOW
time_units       years
distance_units   meters
co_richards      1.0
calculate_flow  TRUE
!!PUMP
 !Pump 0.001057 injection 1 1 1
 !Pump 0.001057 injection 2 1 1
 !Pump 0.001057 injection 3 1 1

!!PERMEABILITY
permeability_x 1e-30 default
permeability_y 2.95e-13 default
permeability_z 0 default

upstream_weighting TRUE
!!RICHARD
ns_solve FALSE
Richards        TRUE
redistribute_wc TRUE
!!wc_init         0.05
watertable_init 0.1
y_is_vertical   TRUE
!!!!!!! VG parameters!!!!!
vangenuchten_alpha 1.43
vangenuchten_n  1.506
wc_residual     0.001

!!PRESSURE
pressure  0    default
pressure  1.0    zone 1-3 101-101 1-1 fix
END

BOUNDARY_CONDITIONS
x_begin   ambient       flux
x_end     ambient       flux
y_begin   injection     flux
y_end     ambient       flux
END

INITIAL_CONDITIONS
 ambient   1-3 1-100 1-1
END

DISCRETIZATION
distance_units  meters
xzones 3 1.00
yzones 100 0.06
zzones 1 1
END

Condition        injection
units            mol/kg
temperature      8
Tracer           0.1
pH               5.31
!!O2(aq) O2(g)   0.168
END

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
Condition        ambient
units            mol/kg
temperature      12.0
pH               7.24
Tracer           1e-25
!!O2(aq) 1e-25
END

POROSITY
fix_porosity     0.33
porosity_update false
END

PRIMARY_SPECIES
Tracer
H+
!!O2(aq)
END

SECONDARY_SPECIES
OH-
END

!!GASES
!!O2(g)
!!END

TEMPERATURE
set_temperature  25.0
END
