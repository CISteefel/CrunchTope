TITLE
Exp_1.in: HCl injection into Core
END

RUNTIME
time_units         minutes
timestep_max       0.5
timestep_init      0.1
time_tolerance     1
hindmarsh          false
correction_max     2.0
debye-huckel       true
database_sweep     false
speciate_only      false
gimrt              false
graphics           tecplot
solver             gmres
preconditioner     ilu
precondition_level 2
Master_variable    H+
database           datacom.dbs
screen_output      10
fix_saturation     1.00
coordinate         cylindrical
END
 
OUTPUT
time_units           minutes
spatial_profile      1 10  60  120  180  240  300  600 900 1200
output_variables     porosity   
time_series          TracerDispersion3.out  1 1 10
time_series_print    Ca++ pH
time_series_interval 1
END

TRANSPORT
distance_units   meters
time_units       seconds
!calculate_diffusion      9.31e-09
fix_diffusion    9.31e-09
D_25  H+        9.31e-09
D_25  Cl-       2.03e-09
D_25  HCO3-     1.18e-09
D_25  Ca++      7.93e-10
dispersivity 0.0002 0.00002
END


BOUNDARY_CONDITIONS
x_begin   initial            flux
x_end     initial            flux
y_begin   initial            flux
y_end     initial            flux
z_begin   initial            flux
z_end     initial            flux
END

INITIAL_CONDITIONS
initial   1-20 1-50 1-20
boundary  12-14 1-50 1-1 fix
END

Condition boundary
units            mol/kg
temperature      25.0
H+               0.05
Cl-              0.05
Ca++             0.0
HCO3-            0.0
Calcite          0.8 specific_surface_area 5.0
END

Condition initial
units            mol/kg
temperature      25.0
H+               1E-07
Cl-              0.0
Ca++             charge
HCO3-            Calcite
Calcite          0.8 specific_surface_area 5.0
END 

FLOW
time_units       seconds
distance_units   meters
!constant_flow 0.0 0.0 3.55E-06
calculate_flow   true
permeability_x      1.4E-13  default
permeability_y      1.4E-13  default
permeability_z      1.4E-13  default
pressure     101325  default
pressure     107588    zone    0-21  0-51  0-0 fix
pressure     101325    zone    0-21  0-51  21-21 fix
END


POROSITY
porosity_update true
END

PRIMARY_SPECIES
H+
Cl-
Ca++
HCO3-
END

MINERALS
Calcite  -label h+
END


SECONDARY_SPECIES
CO2(aq)                                                                 
CO3--                                                                   
CaCO3(aq)                                                               
CaCl+                                                                   
CaCl2(aq)                                                               
CaHCO3+                                                                 
CaOH+                                                                   
HCl(aq)                                                                 
OH-                                                                     
END

DISCRETIZATION
distance_units   cm
xzones 20 0.19 
yzones 50 0.1282
zzones 20 1.25
END