TITLE
sandstone (101.4bar-53C)
END
 
DATABASE
dcpep_Gaby7.dbs
END
 
RUNTIME
!!  Output files will be in same time units
time_units          hours
timestep_max        1.0
timestep_init       2.4e-12
time_tolerance      0.001
lag_activity        true
debye-huckel        true
database_sweep      false
speciate_only       false
gimrt true
modflow false
screen_output 100
coordinate rectangular
master  h+
hindmarsh false
END

OUTPUT
time_units         hours
spatial_profile    10.0 48.0 72.0 96.0 120.0
time_series        outlet.out 30 40
!!  "all" writes all total concentrations, pH, and then all species concentrations
!!  Total concentrations in molality, species in log molality
!!  Listing of primary species will give total concentrations for that species only
time_series_print  pH Ca++ Mg++ HCO3- SO4-- K+ SiO2(aq) Na+ Fe++ Al+++  O2(aq)  Li+ Sr++
time_series_interval 5
END
 
POROSITY
!fix_porosity      0.25
porosity_update   true
END
 
TEMPERATURE
!!read_temperature   filename
set_temperature  53.0
END

