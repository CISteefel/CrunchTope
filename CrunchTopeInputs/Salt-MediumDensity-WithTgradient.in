TITLE
Salt grain pack with temperature gradient
END

DATABASE
datacom.dbs
END

RUNTIME
time_units          hours
timestep_max        10.0
timestep_init       1.e-05
corrmax             10.0
time_tolerance      0.001
debye-huckel        true
database_sweep      false
speciate_only       false
lag_activity        true
gimrt               true
graphics            tecplot
master_variable     Na+
screen_output       10
nmm                 true
SaltCreep           true
ResidualTolerance   1.0E-09
SetSurfaceAreaConstant  true
!!!DampRateInLowPorosity  0.01
END

OUTPUT
time_units               hours
spatial_profile_at_time  0.00001  1.0 10.0 100.0  1000.0 5000.0  10000.0
time_series_at_node      halite.out  1  1
time_series_print        Na+  Cl-
time_series_interval     1  
END

POROSITY
!!!fix_porosity  0.10
porosity_update  true
END

MINERALS
Halite            -label default  -rate  -0.578396073
Bogusite          -label default  -rate  -30.00
END

GASES
END

Condition        porezone    
temperature     25.0
pressure        10.0
set_porosity    0.999
units           mol/kg
Na+             Halite
Cl-             charge
Tracer          1.0E-20
Bogus           Bogusite
Halite          0.00  ssa 0.0
Bogusite        0.0000  ssa 0.00
END

Condition       Halite
temperature     25.0
pressure        10.0
set_porosity    1.0E-05
units           mol/kg
Na+             Halite
Cl-             charge
Tracer          1.0E-20
Bogus           Bogusite
Halite          0.99999  bsa 0.1
!!!Halite          0.0100  bsa 0.01
Bogusite        0.00000  ssa 0.00
END

Condition       LoadingPlate
temperature     25.0
pressure        10.0
set_porosity    1.0E-05
units           mol/kg
Na+             Halite
Cl-             charge
Tracer          1.0E-20
Bogus           Bogusite
Halite           0.00  ssa 0.0
Bogusite        0.99999  ssa 0.00
END

Condition        boundary    
temperature     25.0
pressure        10.0
set_porosity    0.999
units           mol/kg
Na+             Halite
Cl-             charge
Tracer          1.0E-03
Bogus           Bogusite
Halite          0.00    ssa 0.00
Bogusite        0.0000  ssa 0.00
END


BOUNDARY_CONDITIONS
X_begin   boundary  Flux
X_end     porezone  Flux 
Y_begin   porezone  Flux 
Y_end     porezone  Flux 
END


INITIAL_CONDITIONS
halite    1-200  1-120  1-1
porezone  90-110  55-65  1-1
!!!readinitialconditions  Salt-MediumDensity-30Ksteps.dat
END
 
TRANSPORT
distance_units        meters
time_units            second
cementation_exponent  4.0
fix_diffusion         1.3E-09
meandiffusion         ArithmeticMean
END

FLOW
time_units          years
calculate_flow      false
constant_flow       0.0  0.0
END

DISCRETIZATION 
distance_units   mm
xzones  200   0.01
yzones  120   0.01
END

TEMPERATURE
set_temperature  26.0
temperature_gradient  -500
!!!!Read_TemperatureFile  temperature_gradient_salt.dat FullForm   
END

PRIMARY_SPECIES
Na+
Cl-
Tracer
Bogus
END

SECONDARY_SPECIES
!!!NaCl(aq)
END



