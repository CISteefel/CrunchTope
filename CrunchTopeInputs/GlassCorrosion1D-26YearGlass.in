TITLE
26 year French glass
END

RUNTIME
time_units         days
timestep_max       0.005
timestep_init      1.E-10
time_tolerance     0.8
correction_max     1.0
debye-huckel       false
database_sweep     false
speciate_only      false
graphics           kaleidagraph
solver             gmres
preconditioner     bjacobi
precondition_level 0
master_variable    Na+
database           datacomGlass.dbs
coordinates        rectangular
gimrt              true
screen_output      1000
Inagaki            false
InagakiDensify     false
hindmarsh          true
ResidualTolerance  1.0D-07
gimrt_solver       gmres
gimrt_pc           bjacobi
!!gimrt_pc           lu
!!gimrt_pc           ilu
gimrt_pclevel      0
gimrt_rtolksp      1.0E-07
save_restart       Glass-SON68.rst 
!!restart            SON68.rst
END

OUTPUT
time_units           years
spatial_profile     1.00 2.00 3.00  
!!4.00  5.00 
!!!10.00  15.00  20.00  25.75
time_series_at_node      Effluent.out  5  1  1
time_series_print        pH  SiO2(aq)  B(OH)3(aq)  Na+  Cs+
time_series_interval     1
END

DISCRETIZATION 
distance_units  nanometers
!!xzones          22250  2.0
xzones          200  1.0
END

INITIAL_CONDITIONS
VolvicWater               1-20 
Connector                 21-25
Leaching                  26-50
Glass                     51-200
!!!Glass                     26-22250
END

MINERALS
!!  25C rates
!!!JapaneseGlass                   -label default  -rate  -0.70   -cross_affinity SiO2(am-glass2)
