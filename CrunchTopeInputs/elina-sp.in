TITLE
Elina
END
 
DATABASE
dcpep.dbs
END
 
RUNTIME
!!  Output files will be in same time units
time_units          days
timestep_max        1.0e-3
timestep_init       1.0e-8
time_tolerance      0.0001
lag_activity        true
debye-huckel        true
database_sweep      true
speciate_only       true
hindmarsh           true
gimrt               true
modflow             false
screen_output 250
coordinate rectangular
!master H+
END
 
OUTPUT
time_units         days
spatial_profile    365.0 1825.0 3650.0 5475.0
time_series        hole.out 1
!!  "all" writes all total concentrations, pH, and then all species concentrations
!!  Total concentrations in molality, species in log molality
!!  Listing of primary species will give total concentrations for that species only
time_series_print  trac pH Ca++ SiO2(aq) Al+++ K+ Na+ SO4-- Mg++ Cl- F- HCO3-
time_series_interval 10000
END
 
POROSITY
fix_porosity      0.1
!porosity_update   true
END
 
TEMPERATURE
!!read_temperature   filename
set_temperature  25.0
END
 
