TITLE
aperture map degradation.
END

RUNTIME
time_units          hours
time_tolerance      0.001
timestep_max        0.01
gimrt               true
debye-huckel        true
screen_output       1
database            datacom.dbs
!speciate_only       true
!Duan                true
master              Tracer
END
 
OUTPUT
time_units           hours
spatial_profile      0.001 0.01 0.1  
!time_series          breakthrough.out  40 100 1
!time_series_print     pH Ca++ CO2(aq) SiO2(aq)
END
 

PRIMARY_SPECIES
Tracer
END

SECONDARY_SPECIES
END

Condition        Inlet 
units            mol/kg
temperature      25.0

Tracer         0.1 
END

Condition        Outlet 
units            mol/kg
temperature      25.0

Tracer         1.0E-6 
END

Condition        fracture 
units            mol/kg
temperature      25.0
Tracer         1.0E-6 
END

 
TRANSPORT
distance_units centimeters
time_units     seconds
fix_diffusion  0.5E-35
!cementation_exponent  3.1
dispersivity  0.0 0.0

END
 


BOUNDARY_CONDITIONS
x_begin   fracture    flux
x_end     fracture    flux
y_begin   inlet       flux
y_end     outlet      flux
END
 
INITIAL_CONDITIONS
fracture    1-78 1-183 1-1
END
 
POROSITY
fix_porosity    1.0
END
 

DISCRETIZATION
distance_units  millimeters
xzones  78   0.3
yzones  183   0.3
END
 

FLOW
time_units       seconds
distance_units   meters
constant_flow    0.0 1e-4 
!read_VelocityFile   inputVelocity_300um
pressure            0.000      default
END
 
TEMPERATURE
set_temperature  25.0
! may need to change
END

!MINERALS
! rate constant log10(rate, units mol/m^2/s), quartz from Jefferson94
! Calcite constant rate from Chou et al.,1989, 
!Quartz             -label default       -rate   -14.00
!Calcite            -label h+            -rate    0.49
!Calcite            -label default       -rate   -36.19
!Calcite            -label h2co3         -rate   -3.30
!END 

!GASES
!CO2(g)
!END