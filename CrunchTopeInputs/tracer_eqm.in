TITLE
test case for constant Kd problem
END

DATABASE
Andra_v9_TR_Kd.dbs
END

RUNTIME
time_units      years
timestep_max    1.e-01
timestep_init   1.e-05
time_tolerance  0.001
correction_max  2.0
debye-huckel    true
database_sweep  false
gimrt           true
!restart         tracer_1d_eqm.rst append
save_restart    tracer_1d_eqm.rst
graphics        tecplot
speciate_only   true
screen_output   10
END

OUTPUT
END

