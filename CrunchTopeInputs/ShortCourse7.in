TITLE
CO2 attack on wellbore cement
END

DATABASE
datacom_QL-619.dbs
END

RUNTIME
time_units      days
timestep_max    0.0005
timestep_init   0.00000001
time_tolerance  0.0001
correction_max  2.0
hindmarsh       true
debye-huckel    true
database_sweep  false
master           H+
speciate_only   false
gimrt           true
screen_output   1000
!!!fix_saturation  0.999
read_saturationfile QL_Saturation.dat
graphics        tecplot
Duan            true
Qingyun         true
END

OUTPUT
time_units               days
spatial_profile          0.001 5.0  10.0 20.0
time_series_at_node      CO2_GCS  1  1  1
time_series_print        pH
time_series_interval     1  
END

POROSITY
!!fix_porosity        0.3
porosity_update     true
!!!minimum_porosity    0.000000001
!!!porosity_threshold  0.000000001
minimum_porosity    0.013
porosity_threshold  0.013
porosity_exponent   3.00
END

MINERALS
!!!Calcite        -label nucleatecalcite1
Calcite        -label nucleatecalcite2
Calcite        -label h+        -rate   -0.5
Calcite        -label default   -rate -6.7
SiO2(am)                        -rate  -8
Portlandite      -label default -rate  -4.7
CSH(1.6)         -rate  -4.5
Bogusite         -rate  -35.00  
Teflon           -rate  -40.00
END

NUCLEATION

&Nucleation
  NameMineral      = 'Calcite'
  label            = 'nucleatecalcite1'
  A_zero25C        = 0.0000000000001
  B_nucleation     = 0.000905
  Sigma_mJm2       = 47.0
  SSA_m2g          = 1.2 
  Surface          = 'Calcite'
  /

&Nucleation
  NameMineral      = Calcite
  label            = nucleatecalcite2
