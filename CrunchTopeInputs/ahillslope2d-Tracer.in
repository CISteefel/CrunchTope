TITLE
Chronosequence Be with new surface complexation, laboratory rates
END
 
RUNTIME
time_units        years
timestep_max      0.1
timestep_init     1.0E-14
time_tolerance    0.005
correction_max    2.0
debye-huckel      true
speciate_only     false
database_sweep    false
database          datacomBe14.dbs
graphics          tecplot
screen_output     100
save_restart     ahillslope2d.rst
later_inputfiles  ahillslope2da-Tracer.in 
gimrt         true
solvermethod  bicg
pclevel       3
pc            ilu
initialize_hydrostatic   false
END

FLOW
time_units      years
distance_units  meters
calculate_flow       true
read_permeabilityfile   perm 
!!!!permeability_X      1.5e-13  
!!!!permeability_Y      1.5e-13 
!!!pressure            100000 zone 0-0 1-80 1-1 fix 
pressure            0 zone 101-101 1-80  1-1 fix      
pressure           0 zone 0-101  81-81 1-1 fix     
gravity    90.0  90.0  90.0 up
END
 
MINERALS  
END
 
GASES
END
 
 
TEMPERATURE
set_temperature 25
END
 
DISCRETIZATION
xzones 100 .5
yzones 80 .25
END
 
BOUNDARY_CONDITIONS
X_begin   Profile        flux
X_end     Profile        flux
Y_begin   Profile        flux
Y_end     Profile        flux
END
 
INITIAL_CONDITIONS
Profile 1-100 1-79 1-1
Pulse 1-100 79-80 1-1
!Profile 1-70 19-19 1-1
END
 
TRANSPORT
distance_units       centimeters
time_units           seconds
fix_diffusion        0 
formation_factor     5.0
!cementation_exponent  1.3
!dispersivity          0 0
END
 
 
PRIMARY_SPECIES
Tracer
END
 
Condition Pulse
temperature    25
Tracer                             1E-05
END


Condition Profile  !Water at 1.2 m
temperature    25
Tracer                             1E-05
END

SECONDARY_SPECIES
END


POROSITY
fix_porosity   0.30
porosity_update false
END


OUTPUT 
spatial_profile      5 
time_series          discharge18.txt  50 18 1
time_series          discharge19.txt  50 19 1
time_series          discharge20.txt  50 20 1
time_series          discharge21.txt  50 21 1
time_series          discharge22.txt  50 22 1
time_series          discharge23.txt  50 23 1
time_series          discharge24.txt  50 24 1
time_series          discharge25.txt  50 25 1
time_series          discharge26.txt  50 26 1
time_series          discharge27.txt  50 27 1
time_series          discharge28.txt  50 28 1
time_series          discharge29.txt  50 29 1
time_series_print    Tracer
END