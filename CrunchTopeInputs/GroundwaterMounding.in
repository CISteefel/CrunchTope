TITLE
Test groundwater mounding
END
 
RUNTIME
time_units        years
timestep_max      0.01
timestep_init     1.0E-14
time_tolerance    0.005
correction_max    2.0
debye-huckel      true
speciate_only     false
database_sweep    false
database          datacomBe14.dbs
graphics          tecplot
screen_output     100
master            Tracer
save_restart      GroundwaterMounding.rst
!!!later_inputfiles  GroundwaterMounding-b.in
gimrt         true
solvermethod  bcgs
pclevel       10
pc            ilu
initialize_hydrostatic   false
END

OUTPUT 
spatial_profile      0.01  0.1  1.0
time_series_print    Tracer 
END

FLOW
time_units      years
distance_units  meters
permeability_X      1.5e-14  
permeability_Y      1.5e-14 
permeability_Y      0.000    zone  1-100    0-0 1-1
!!!permeability_X      0.000    zone  0-0     1-80 1-1
!!!permeability_X      0.000    zone  101-101 1-80 1-1
calculate_flow       true
!!read_permeabilityfile perm 
!14 to 19 is low perm zone
pressure            0.0      zone 101-101  70-80  1-1 fix      
pressure            0.0      zone     0-0  70-80  1-1 fix         
pump    0.000525     Pulse     50  70  1 
pump    0.000525     Pulse     51  70  1 
!!!gravity          110.0  20.0  90.0 up
gravity          90.0  0.0  90.0 up
END
 
MINERALS      
END
 
GASES
END
 
TEMPERATURE
set_temperature 25
END
 
DISCRETIZATION
xzones 100 .5
yzones 80 .25
END
 
BOUNDARY_CONDITIONS
X_begin   Profile        flux
X_end     Profile        flux
Y_begin   Profile        flux
Y_end     Profile        flux
END
 
INITIAL_CONDITIONS
Profile 1-100 1-79 1-1
Profile 1-100 79-80 1-1
!Profile 1-70 19-19 1-1
END
 
TRANSPORT
distance_units       centimeters
time_units           seconds
fix_diffusion        0.919E-05
formation_factor     5.0
!cementation_exponent  1.3
!dispersivity          0 0
END
 
PRIMARY_SPECIES
Tracer
END
 
Condition Pulse
temperature    25
Tracer                             1E-02
END

Condition Profile  !Water at 1.2 m
temperature    25
Tracer                             1E-08
END

SECONDARY_SPECIES
END


POROSITY
fix_porosity   0.30
porosity_update false
END

