TITLE
Albite precipitation test
END

RUNTIME
time_units     minutes
timestep_max   1e-2
timestep_init  1e-10
time_tolerance 0.001 
hindmarsh      false
correction_max 2.0
gimrt          true
gimrt_pc       ilu   !default_bjacobi or ilu
gimrt_solver   bcgs  !default_gmres or bcgs
database       datacom-Mansor.dbs
screen_output  100
END

DISCRETIZATION
distance_units centimeters
xzones  1  1   
END

INITIAL_CONDITIONS
plug   1
END

BOUNDARY_CONDITIONS
x_begin plug  flux
x_end   plug  flux
END

TEMPERATURE
set_temperature  25
END

POROSITY
END

PRIMARY_SPECIES
H+
SiO2(aq)
Al+++
Na+
END

SECONDARY_SPECIES
END

MINERALS
!Albite -label dissolutiononly
Albite -label HellmannFFE
!Albite -label HellmannCTE
END

CONDITION plug
units mmol/kg
pH 6.5
Na+       0
Al+++     0
SiO2(aq)  0
Albite    0.1
END

OUTPUT
time_units        minutes
spatial_profile   75  
time_series       Antic.out 1
time_series_print pH Na+  Al+++ SiO2(aq)
time_series_output  1 5 10 15 20 25 50 75  
END




