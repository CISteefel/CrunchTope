TITLE
Test of species diffusion against Lichtner (MULTIFLO) example
END
 
DATABASE
datacom.dbs  
END
 
RUNTIME
time_units      days
timestep_max    1.0e0
timestep_init   1.0e-10
time_tolerance  0.1
correction_max  1.0
debye-huckel    no
database_sweep  no
gimrt           true
!!giambalvo		true
!!gimrt_pc		ilu
!!gimrt_pclevel	10
!!gimrt_solver	bcgs
!!hindmarsh		true
lag_activity    false
graphics        tecplot
screen_output   100
coordinate       cylindrical
END
 
OUTPUT
time_units       days
spatial_profile  40 
!!80 1000
!!time_series          pH_Brtrgh.out 5
!!time_series_print    pH  
time_series_interval 1 
END

INITIAL_CONDITIONS
initial 1-100
inlet   1-1     fix
END
 
BOUNDARY_CONDITIONS
X_begin   inlet        Dirichlet
X_end     outlet       flux
END
 
TRANSPORT
distance_units centimeters
time_units  seconds
fix_diffusion  2.0e-05
!!formation_factor  1.0  
cementation_exponent  1.0
D_25  H+      9.311e-05
D_25  Na+     1.334e-05
D_25  Cl-     2.032e-05
D_25  NO3-    1.902e-05
D_25  OH-     5.273e-05
END

Condition initial
temperature      25.0
pH               6.0
Cl-              1.e-04 
Na+              1.e-04
NO3-             1.e-06
END

Condition inlet
temperature      25.0
!!set_porosity     3.00
pH               10.0
Cl-              1.e-04 
Na+              1.e-04
NO3-             1.e-10
END

Condition outlet
temperature      25.0
pH               6.0
Cl-              1.e-04 
Na+              1.e-04
NO3-             1.e-06
END

DISCRETIZATION
distance_units centimeters
xzones 100 1
END

POROSITY
fix_porosity     0.25  
porosity_update  false
END
 
TEMPERATURE
set_temperature  25.0
END
 
PRIMARY_SPECIES
H+
Cl-
Na+
NO3-
END
 
SECONDARY_SPECIES
OH-
END
 
FLOW
time_units  seconds
distance_units meters
!!constant_flow  2.5E-8
END
























