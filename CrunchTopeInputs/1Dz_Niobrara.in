MINERALS
! Dolomite coefficients from Gautelier 2000 
! Calcite coefficients from Deng et al.,2015, 
! other minerals from Beckingham et al 2016
Dolomite             -label Gautelier-h+       -rate   -3.0079 

Calcite            -label leichou1            -rate    -1.0809 
Calcite            -label leichou3       -rate   -4.8239 
Calcite            -label leichou2         -rate   -3.9586 

Illite            -label default       -rate   -12.6
Smectite-low-Fe-Mg          -label default       -rate   -13.11
Quartz            -label default       -rate   -11.48
PlagAn50Ab50      -label default      -rate   -10.68
Pyrite            -label default       -rate   -7.9
END 

GASES
CO2(g)
END

 
TRANSPORT
distance_units centimeters
time_units     seconds
fix_diffusion  1.0E-5
!cementation_exponent  3.1
dispersivity  0.0 0.0

END
 


BOUNDARY_CONDITIONS
x_begin   inlet    Dirichlet
x_end     matrix    flux
y_begin   matrix    flux
y_end     matrix    flux
END
 
INITIAL_CONDITIONS
brine       1-1 1-1 1-1
matrix      2-500 1-1 1-1
END
 
POROSITY
porosity_update  true
END
 

DISCRETIZATION
distance_units  millimeters
xzones  500 0.004 
yzones  1   0.054
zzones  1  0.054
END
 

FLOW
time_units       seconds
distance_units   meters
constant_flow    0.0  0.0

END
 
TEMPERATURE
set_temperature  25.0
! may need to change
END
 

