TITLE
aperture map degradation.
END

RUNTIME
time_units          hours
timestep_max    0.0001
timestep_init   1.0E-10
time_tolerance  0.01
correction_max  10.0
time_tolerance      0.001
gimrt               true
debye-huckel        true
screen_output       50
database            datacom.dbs
graphics        tecplot
master          Tracer
gimrt_solver    bcgs
gimrt_pc        ilu
gimrt_pclevel   1
END
 
OUTPUT
time_units           hours
spatial_profile      1.0E-10  2.0E-05 4.0E-05 6.0E-05 8.0E-05 10.0E-05 0.0002 0.0003
!time_series          breakthrough.out  40 100 1
!time_series_print     pH Ca++ CO2(aq) SiO2(aq)
END
 
PRIMARY_SPECIES
Tracer
END

SECONDARY_SPECIES
END

Condition        Inlet 
units            mol/kg
temperature      25.0
Tracer           0.01 
END

Condition        Outlet 
units            mol/kg
temperature      25.0
Tracer           0.000001 
END

Condition        fracture 
units            mol/kg
temperature      25.0
Tracer           0.000001
END
 
TRANSPORT
distance_units centimeters
time_units     seconds
fix_diffusion  0.5E-5
cementation_exponent  3.1
dispersivity  0.0 0.0
END
 
BOUNDARY_CONDITIONS
x_begin   fracture    flux
x_end     fracture    flux
y_begin   inlet       flux
y_end     outlet      flux
END
 
INITIAL_CONDITIONS
fracture    1-78 1-183 1-1
END
 
POROSITY
fix_porosity    1.0
END
 

DISCRETIZATION
distance_units  millimeters
xzones  78   0.3
yzones  183   0.3
END
 

FLOW
time_units       seconds
distance_units   meters
read_VelocityFile   inputVelocity_300um
END
 
TEMPERATURE
set_temperature  25.0
! may need to change
END
 

