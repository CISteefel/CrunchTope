TITLE
Test gas diffusion
END

DATABASE
datacom.dbs
END

RUNTIME
time_units      minutes
timestep_max    0.1
timestep_init   1E-10
time_tolerance  0.001
correction_max  2.0
hindmarsh       true
debye-huckel    true
database_sweep  false
master          H+
speciate_only   false
gimrt           true
screen_output   1000
!!!fix_saturation  0.8
graphics        kaleidagraph
Duan            false
Benchmark       false
Qingyun         false
END

OUTPUT
time_units               minutes
spatial_profile          10000.0
time_series_at_node      TS1.out  1  1  1
time_series_at_node      TS2.out  10  1  1
time_series_at_node      TS3.out  100  1  1
time_series_print        pH
time_series_interval     1  
END

POROSITY
fix_porosity 0.135
porosity_update false
END

TEMPERATURE
set_temperature 25.0
END

PRIMARY_SPECIES
H+
CO2(aq) 
Tracer
END

SECONDARY_SPECIES
HCO3-                                                                                                                         
OH-                                                                     
CO3--                                                                                                                            
END

GASES
CO2(g)
Tracer(g)
END

MINERALS 
Cdiox    -label default  -rate  -8.0
END

CONDITION Bnd
units            mol/kg
pH               charge
CO2(aq)          CO2(g)  3.1623e-04
Tracer           0.1
Cdiox            0.9     ssa 1.0
END

CONDITION Cement
units            mol/kg
pH               charge
CO2(aq)          CO2(g) 1.00E-06
Tracer           0.000001
Cdiox            0.001  ssa 1.0
END

DISCRETIZATION 
distance_units  millimeters
xzones          100  0.1
END

INITIAL_CONDITIONS
Cement        1-20
Bnd           21-80
Cement        81-100
END

BOUNDARY_CONDITIONS
X_begin     Cement       Dirichlet 
X_end       Cement       Dirichlet 
END

TRANSPORT
distance_units        meters
time_units            second
cementation_exponent  0
gas_diffusion         1.10E-6
fix_diffusion         1.0E-100
END

FLOW
distance_units      millimeters
time_units          second
constant_flow       0.0
calculate_flow      false
END

