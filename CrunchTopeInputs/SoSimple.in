TITLE
Acetate injection to an initial condition similar to the Rifle aquifer
END
 
RUNTIME
time_units              days
timestep_max            0.01
timestep_init           1.E-11
time_tolerance          0.001
hindmarsh               true
correction_max          2.0
debye-huckel            true
database_sweep          false
speciate_only           true
graphics                kaleidagraph
!!!graphics                originlab
master                  H+
database                datacom.dbs
coordinates             rectangular
gimrt                   false
screen_output           100
END
 
OUTPUT
time_units  days
spatial_profile       2
time_series           0cm.out          1  1  1
time_series           100cm.out        50  1  1
time_series           200cm.out        100  1  1
time_series_print     Na+ Br- pH
time_series_units     mol/kgw
time_series_interval  10
END

condition   groundwater
units            mmol/kg
temperature      25.0
!!pH               7.0
H+   charge            
!!!6.3096E-08
Na+              1.0E-15
Br-              1.0E-15
HCO3-            1.00E-3
END
 
INITIAL_CONDITIONS
groundwater   1-100
END
 
BOUNDARY_CONDITIONS
x_begin   groundwater   flux
x_end     groundwater   flux
END
 
DISCRETIZATION
distance_units      centimeters
xzones              100  2.0
END
 
FLOW
distance_units      centimeters
time_units          days
constant_flow     0.0
END
 
POROSITY
fix_porosity 0.25
END
 
TRANSPORT
distance_units           centimeters
time_units               second
fix_diffusion            0.919e-05
END

PRIMARY_SPECIES
H+
Br-
Na+
HCO3-
END
 
SECONDARY_SPECIES
OH-
CO3--
CO2(aq)
END
