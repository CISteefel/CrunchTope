END

GASES
!!O2(g)
H2(g)
!!CO2(g)
!!H2S(g)
!!NH3(g)
END

OUTPUT
time_units            years
spatial_profile       0.00001 75000 200000 500000
!!spatial_profile       6500 4600 8900
!!time_series           timeseries_Z10.txt 10
!!time_series           10cm.txt 1000
!!time_series           25cm.txt 2500
!!time_series           50cm.txt 5000
!!time_series           100cm.txt 10000
!!time_series           200cm.txt 20000
!!time_series_print     pH HCO3- O2(aq) NO3- SO4-- Acetate Ca40++ Ca44++
!!time_series_print     pH Ca40++ Ca44++ HCO3- Mg++ Sr++ Na+ Cl- Acetate O2(aq) NO3- SO4--
!!time_series_interval  1
!!MakeMovie             true pH
END

MINERALS
Kaolinite            -label preciponly       -rate -80.00
Calcite40            -label preciponly       -rate -11.24
Calcite44            -label preciponly       -rate -11.24
CalciteIsoDiss       -label dissolutiononly  -rate -6.00
!!Celestite            -label default        -rate -6.00
C5H7O2NO2(s)         -label default        -rate -60.0
C5H7O2NNO3(s)        -label default        -rate -60.0
C5H7O2NSO4(s)        -label tst            -rate -60.0
Acetate(c)           -label default        -rate -20.0
!!Lactate(c)           -label default        -rate -20.4
Quartz               -label default        -rate -60.0
!!decay_b_so4(s)       -label default        -rate -9.0   -associate  C5H7O2NSO4(s)
END

DISCRETIZATION
distance_units meters
xzones   200   0.1
END

INITIAL_CONDITIONS
initcolcarb90    1-200
END

BOUNDARY_CONDITIONS
X_begin   BCcarb90     Dirichlet
X_end     BCcarb90     flux
END

EROSION/BURIAL
distance_units     meters
time_units         years
erode_x            10e-06
END

AQUEOUS_KINETICS
!! rate expression then stoichiometry
!! nitrate reduction/one pathway for simplicities sake
!!-------------------------------------------------------------------------------------------
Aerobic_oxidation_Ac         -pathway Aerobic_oxidation_Ac    1.0    -rate -20.0
Nitrate_reduction_inhib_Ac   -pathway Nitrate_reduction_Ac    1.0    -rate -20.0
Sulfate_reduction_noFe_Ac    -pathway Sulfate_reduction_Ac    1.0    -rate 0.1
HS_oxidto_SO4                -pathway HS_oxidto_SO4           1.0    -rate 0.01
!!
!! INIT PARAMETERS
!!
!!Aerobic_oxidation            -pathway Aerobic_oxidation_half         1.0   -rate 1.0
!!Nitrate_reduction_inhib_hw5  -pathway Nitrate_reduction_inhib_half   1.0   -rate 1.0
!!Sulfate_reduction_inhib_hw5  -pathway Sulfate_reduction_inhib_half   1.0   -rate 1.0
!!HS_oxidto_SO4                -pathway HS_oxidto_SO4                  1.0   -rate 0.5
!!
!!Aerobic_oxidation            -pathway Aerobic_oxidation_half         0.40  -pathway C5H7O2N_Ace_NH4_O2   0.60   -rate 20
!!Nitrate_reduction_inhib_hw5  -pathway Nitrate_reduction_inhib_half   0.45  -pathway C5H7O2N_Ace_NH4_NO3  0.55   -rate 40
!!Sulfate_reduction_inhib_hw5  -pathway Sulfate_reduction_inhib_half   0.92  -pathway C5H7O2N_Ace_NH4_SO4  0.08   -rate 100
!!H2S_oxidto_SO4               -pathway H2S_oxidto_SO4                 1.0   -rate 0.01
!! init rates tried: 50000; 20000; 37500// SR 0.92-0.08
!!-------------------------------------------------------------------------------------------
END

