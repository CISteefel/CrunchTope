TITLE
Mont Terri Cl-D Experiment
END

DATABASE
datacom.dbs
END

RUNTIME
time_units          hours
timestep_max        100.0
timestep_init       1.e-05
corrmax             10.0
time_tolerance      0.001
debye-huckel        true
database_sweep      false
speciate_only       false
lag_activity        true
gimrt               false
graphics            tecplot
master_variable     Tracer
screen_output       10
nmm                 false
ResidualTolerance   1.0E-09
MontTerri           true
END

OUTPUT
time_units               days
spatial_profile_at_time  1.0 10.0 50.0 100.0 300.0 1000.0  3650 4380
!!!0.7 0.8 0.9 1.0  10.0  12.0 
time_series_at_node      Cl-D.out  1  1 1
time_series_print        Tracer
time_series_interval     1  
END

POROSITY
fix_porosity  0.3
!!!porosity_update  true
END

MINERALS
Bogusite          -label default  -rate  -30.00
END

GASES
END

DISCRETIZATION 
distance_units   cm
xzones  60       1.0
yzones  60       1.0
zzones  100      1.0
END

!!! 1: Chamber
!!! 2: Not in Chamber
!!! 3: Cement
!!! 4: Skin
!!! 5: Outer disturbed zone
!!! 6: Inner disturbed zone
!!! 7: Opalinus Clay (OPA)

Condition       chamber  
units           mol/kg
temperature     25.0
set_porosity    2.00
Tracer          0.1
Bogus           1.00
Bogusite        0.0000  ssa 0.00
END

Condition       notinchamber  
units           mol/kg
temperature     25.0
set_porosity    0.3
Tracer          0.000001
Bogus           0.0001
Bogusite        0.0000  ssa 0.00
END

Condition       cement  
units           mol/kg
temperature     25.0
set_porosity    0.24
Tracer          0.000001
Bogus           0.0001
Bogusite        0.0000  ssa 0.00
END

Condition       skin  
units           mol/kg
temperature     25.0
set_porosity    0.24
Tracer          0.000001
Bogus           0.0001
Bogusite        0.0000  ssa 0.00
END

Condition       outer  
units           mol/kg
temperature     25.0
set_porosity    0.12
Tracer          0.000001
Bogus           0.0001
Bogusite        0.0000  ssa 0.00
END

Condition       inner  
units           mol/kg
temperature     25.0
set_porosity    0.12
Tracer          0.000001
Bogus           0.0001
Bogusite        0.0000  ssa 0.00
END

Condition       Opa  
units           mol/kg
temperature     25.0
set_porosity    0.12
Tracer          0.000001
Bogus           0.0001
Bogusite        0.0000  ssa 0.00
END


BOUNDARY_CONDITIONS
X_begin   opa    Flux
X_end     opa    Flux 
Y_begin   opa    Flux 
Y_end     opa    Flux 
Z_begin   opa    Flux 
Z_end     opa    Flux 
END

INITIAL_CONDITIONS
readinitialconditions  MontTerri_Cl-D.dat
END
 
TRANSPORT
distance_units        meters
time_units            second
cementation_exponent  1.0
fix_diffusion         1.0E-09
meandiffusion         ArithmeticMean
anisotropy_ratioZ    1.0
anisotropy_ratioY    1.0
END

FLOW
time_units          years
calculate_flow      false
constant_flow       0.0  0.0  0.0
END

TEMPERATURE
set_temperature  25.0
END

PRIMARY_SPECIES
Tracer
Bogus
END

SECONDARY_SPECIES
END


