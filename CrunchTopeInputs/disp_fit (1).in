TITLE
Analytical dispersion fit
END

RUNTIME
time_units         days
timestep_max       0.01
timestep_init      0.01
time_tolerance     0.1
correction_max     2.0
debye-huckel       true
speciate_only      false
gimrt              false
graphics           tecplot
solver             gmres
preconditioner     ilu
precondition_level 0
database           datacom.dbs
screen_output      100
END

OUTPUT
time_units               days
time_series              TimeSeries.out 1  1  1
time_series_print        Tracer
time_series_interval     1  
spatial_profile          2.5
END

PRIMARY_SPECIES
Tracer
END

Condition        initial
units            mol/kg
temperature      25.0
Tracer           0.00
END

Condition        tracer
units            mol/kg
temperature      25.0
Tracer           1.00
END

POROSITY
fix_porosity     1.0
END

DISCRETIZATION
distance_units      centimeters
xzones  101   0.5
yzones  59   0.5
END
 
INITIAL_CONDITIONS
initial    1-1   1-59  fix
initial    2-101  1-59
tracer     1-101  30-30  fix
END

BOUNDARY_CONDITIONS
x_begin   initial  flux 
x_end     initial  flux
y_begin   initial  flux
y_end     initial  flux
END

TEMPERATURE
set_temperature  25.0
END

TRANSPORT
distance_units   centimeters
time_units       second
fix_diffusion    0.00
dispersivity     0.75   0.75
END

FLOW
distance_units   centimeters
time_units       second
calculate_flow   true
permeability_x    1e-12  default
permeability_x    1e-15  zone 1-101  30-30  1-1
permeability_y    1e-12  default
permeability_y    1e-15  zone 1-101  30-30  1-1
pressure          1e4  zone 0-0  1-59  1-1  fix
pressure          0  zone 102-102  1-59  1-1  fix
END


