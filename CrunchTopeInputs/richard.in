TITLE 
Richardpseudo1D-pump-lucien 
END 

RUNTIME 
time_units       years 
timestep_max     1.0E-04
timestep_init    1.0E-04 
time_tolerance   0.001 
hindmarsh        false 
correction_max   2.0 
debye-huckel     yes 
master           O2(aq) 
database_sweep   no 
speciate_only    no 
gimrt            true
screen_output     1000
database         datacom_lucien.dbs 
END 

OUTPUT    
time_units               years    
spatial_profile          10  
END 

TRANSPORT 
distance_units meters 
time_units     seconds 
fix_diffusion  1.0e-09 
gas_diffusion  1.0E-26
!!MilligtonQuirk true 
!!dispersivity   0.0 

FLOW 
time_units        years 
distance_units    meters 
!!constant_flow   0 0.3 0 
Pump 9.5129e-06        injection 1 1 1 
Pump 9.5129e-06        injection 2 1 1 
Pump 9.5129e-06        injection 3 1 1 
calculate_flow    TRUE 
!!PERMEABILITY 
permeability_x 2.8e-15   default 
permeability_y 2.8e-15   default 
permeability_z 1.418e-30 default
ns_solve                 FALSE 
Richards                 TRUE 
redistribute_wc          FALSE 
upstream_weighting       TRUE
wc_init                  0.05 
wc_residual              0.01 
watertable_init          0.02 
vangenuchten_alpha       7.5 
vangenuchten_n           1.25 
y_is_vertical            TRUE 
!!PRESSURE 
pressure  0.0    default 
pressure  0.0    zone 1-3 0-0 1-1 fix  
pressure  0.0    zone 1-3 17-17 1-1 fix  
END 

BOUNDARY_CONDITIONS    
x_begin   injection     flux 
x_end     ambient       flux 
y_begin   injection     flux 
y_end     ambient       flux 
END 

INITIAL_CONDITIONS    
ambient   1-3 1-16 1-1 
END 

DISCRETIZATION    
distance_units  meters 
xzones 3  1  
yzones 16 0.5 
zzones 1  1  
END 

Condition        injection    
units            mol/kg 
temperature      25 
Tracer           0.1  
pH               5.31  
O2(aq)           O2(g)    0.168  
END 

Condition        ambient    
units            mol/kg 
temperature      25 
pH               7.24  
Tracer           1.0E-20 
O2(aq)           O2(g)   1.0E-15 
END 

POROSITY    
fix_porosity     0.2    
porosity_update false    
END 

PRIMARY_SPECIES 
Tracer 
H+ 
O2(aq) 
END 

SECONDARY_SPECIES 
OH-  
END 

GASES    
O2(g)    
END 

TEMPERATURE    
set_temperature  25.0    
END 

