END
 

RUNTIME
correction_max         2.0
database               datacom_plusU.dbs
database_sweep         false
debye-huckel           true
density_module         temperature
!dissolution_max       0.001
!fix_saturation        1.0
gimrt                  true
graphics               kaleidagraph
hindmarsh              true
lag_activity           true
screen_output          500
speciate_only          false
time_units             hours
timestep_max           1
timestep_init          0.001
time_tolerance         0.1
!!!generic_rates          -7.0
!!!duan           true 
END 

OUTPUT
time_units                 hours
spatial_profile            190
time_series                eff_KCoC.out   50  1  1
time_series_print          pH UO2++ Na+ Mg++ K+ Ca++   SiO2(aq)  Cl- SO4-- CO2(aq)
time_series_interval       5
time_series_output    0.1 0.8 1.1 1.5 2.0 2.5 4.0 6.7 8.0 9.3 12.7 18.0 22.0 26.0 34.0 42.0 52.7 &
66.0 79.3 92.7 106.0 119.3 132.7 146.0 159.3 175.0 190.0
END

