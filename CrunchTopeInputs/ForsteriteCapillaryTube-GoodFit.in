NUCLEATION
&Nucleation
  NameMineral      = Magnesite1
  label            = nucleate_magnesite1
  A_zero25C        = 0.1
  B_nucleation     = 0.0000905
  Sigma_mJm2       = 150.0
  SSA_m2g          = 100.0
  Surface          = Forsterite
  /
END

TRANSPORT
distance_units  meters
time_units      seconds
calculate_diffusion   1.3e-09
!!!diffusion_activation  2.25
!!!diffusion_activation  2.00
!!!diffusion_activation  1.75
!!!diffusion_activation  2.50
diffusion_activation  2.75
tortuosity      1.0 default
dispersivity    0.0 0.0
D_25  H+       9.0e-09
D_25  Na+      1.3e-09
D_25  Cl-      2.1e-09
D_25  OH-      4.5e-09
D_25  CO2(aq)  1.6e-09

END



INITIAL_CONDITIONS
capillary       1-100  1-1  1-1
END

BOUNDARY_CONDITIONS
X_begin     co2brine        Dirichlet
X_end       capillary       flux
END

TEMPERATURE
set_temperature      95.0
END



POROSITY
fix_porosity   0.500
porosity_update  false
END

FLOW
distance_units    meters
time_units        minutes
constant_flow     0.0
END

DISCRETIZATION
space_units   meter
xzones        100  0.0003
END

Condition capillary
temperature        95.0
set_porosity       0.500
pH                 charge
