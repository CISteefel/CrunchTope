TITLE 
Li Yang's Capillary Tube Forsterite Experiment
END

RUNTIME
time_units      hours
database        Liz-CIS.dbs
database_sweep  false
gimrt           true
timestep_init   1E-9
timestep_max    0.005
screen_output   1000
speciate_only   false
hindmarsh       true
fix_saturation  1.00
duan            true
ForsteriteCapillary  true
graphics        kaleidagraph
save_restart    NoNucleation.rst
END

OUTPUT
time_units   hours
spatial_profile  50.0 100.0 150.0 200.0 300.0
time_series       LiYangCapillary-Forsterite.out  100
time_series_print Tracer Mg++ SiO2(aq)
time_series_interval  1
END


