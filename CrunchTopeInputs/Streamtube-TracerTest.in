TITLE
Streamtube test
END

RUNTIME
time_units      seconds
timestep_max    1.0
timestep_init   0.01
time_tolerance  0.001
correction_max  1.0
debye-huckel    false
database        calcitedata.dbs
master          Tracer
speciate_only   false
database_sweep  false
screen_output   1000
hindmarsh       true
gimrt           true
!!!read_saturation saturation.dat
streamtube  true
END

OUTPUT
time_units  seconds
spatial_profile      100.0  500.0 1000.0
time_series           vprofile.out  10 1 1
time_series_print    Tracer
END


DISCRETIZATION
distance_units  centimeters
xzones  10 1.0
read_volumes   streamtube.vol  FullForm
END

Condition inlet
units        mol/kg
temperature      25.0
Tracer           0.1
set_porosity   0.99
END

Condition effluent
units        mol/kg
temperature      25.0
Tracer           0.1
set_porosity   0.99
END

Condition pore
units      mol/kg
temperature      25.0
Tracer           0.00
set_porosity   0.99
END



INITIAL_CONDITIONS
pore     1-10  1-1 1-1
END

BOUNDARY_CONDITIONS
X_begin   inlet Flux
X_end     inlet Flux
END

POROSITY
fix_porosity     0.99
porosity_update  false
END

TEMPERATURE
set_temperature  25.0
END

FLOW
time_units      seconds     ! these units specify the units of the flow
distance_units  centimeters ! velocity to be cm/s
calculate_flow  FALSE
ns_solve        FALSE
Richards        FALSE
!!!constant_flow   1.0
read_velocityFile   streamtube
END


!!! Geochemistry

PRIMARY_SPECIES
Tracer
END

SECONDARY_SPECIES
END

MINERALS
END

GASES
END


