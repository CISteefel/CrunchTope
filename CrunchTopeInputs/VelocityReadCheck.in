TITLE
Velocity read check
END

DATABASE
datacom.dbs
END

RUNTIME
time_units          years
timestep_max        0.001
timestep_init       0.001
corrmax             2.0
time_tolerance      0.5
debye-huckel        true
database_sweep      false
speciate_only       false
lag_activity        true
gimrt               true
graphics            tecplot
master_variable     Tracer
PC                 ilu
PClevel            2
GIMRT_PC           bjacobi
GIMRT_PClevel      1
GIMRT_solver       gmres
screen_output      1
END

OUTPUT
time_units               years
spatial_profile_at_time  2.0 
time_series_at_node      VelocityReadOutput.out  4 4 1
time_series_print        Tracer
time_series_interval     1  
END

POROSITY
fix_porosity     0.9999
porosity_update  false
END

TEMPERATURE
set_temperature  25.0
END

PRIMARY_SPECIES
Tracer
END

SECONDARY_SPECIES
END


DISCRETIZATION 
xzones  4   1.0 
yzones  4   1.0
END
 
Condition initial_condition Initial condition for simulation 
temperature     25.0
Tracer    0.0000001
END

Condition leach_solution    Composition of leach solution at injection well
temperature     25.0
Tracer    1.000
END
END

INITIAL_CONDITIONS
initial_condition  1-4  1-4    
END

BOUNDARY_CONDITIONS
X_begin   initial_condition  Flux 
X_end     initial_condition  Flux 
Y_begin   initial_condition  Flux 
Y_end     initial_condition  Flux 
END

TRANSPORT
time_units  years
diffusion     0.0
dispersivity  0.0  0.0
END


FLOW
time_units  years
read_velocity VelocityCheck
END
